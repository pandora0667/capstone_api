<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.wisoft.capstone.dao.InterfaceCarDao">
    <resultMap type="car" id="CarResultMap">
        <id column="ID" property="id"/>
        <result column="kind" property="kind"/>
        <result column="licensePlate" property="licensePlate"/>
    </resultMap>
    <insert id="insert" parameterType="car">
        INSERT INTO car VALUES (#{kind}, #{licensePlate}, #{id});
    </insert>
</mapper>
